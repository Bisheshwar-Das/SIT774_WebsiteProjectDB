{
  "name": "sit774_10.3d",
  "version": "1.0.0",
  "main": "server/app.js",
  "scripts": {
    "start": "node server/app.js",
    "start:dev": "nodemon server/app.js",
    "test": "jest --verbose --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "echo 'Build completed successfully'",
    "lint": "eslint server/ --ext .js --max-warnings=0",
    "lint:fix": "eslint server/ --ext .js --fix",
    "security-check": "npm audit"
  },
  "keywords": [
    "express",
    "sqlite",
    "scenarios",
    "web-application"
  ],
  "author": "",
  "license": "ISC",
  "description": "Web application for scenario sharing with database integration",
  "dependencies": {
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.4"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "outputName": "junit.xml"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
